{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/main.js"
    }
  ],
  "functions": {
    "api/**/*.{js,ts}": {
      "runtime": "bun@latest"
    }
  },
  "framework": null,
  "cleanUrls": true,
  "devCommand": "bun run start:dev",
  "installCommand": "bun install",
  "buildCommand": "bun run build",
  "outputDirectory": "dist"
}