{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "functions": {
    "api/main.ts": {
      "runtime": "nodejs20.x",
      "memory": 512,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/main"
    }
  ],
  "framework": null,
  "cleanUrls": true,
  "devCommand": "bun run start:dev",
  "installCommand": "bun install",
  "buildCommand": "bun run build",
  "outputDirectory": "dist"
}