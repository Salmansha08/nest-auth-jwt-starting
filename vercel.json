{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "buildCommand": "bun run build",
  "devCommand": "bun run start:dev",
  "framework": null,
  "functions": {
    "src/*.ts": {
      "runtime": "nodejs",
      "memory": 3009,
      "maxDuration": 60
    }
  },
  "installCommand": "bun install",
  "outputDirectory": "dist",
  "redirects": [
    {
      "source": "/",
      "destination": "/api/docs",
      "permanent": false
    }
  ],
  "regions": [
    "sin1"
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/main.ts"
    }
  ],
  "trailingSlash": false
}