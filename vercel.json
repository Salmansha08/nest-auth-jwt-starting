{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "crons": [],
  "framework": null,
  "devCommand": "bun run start:dev",
  "installCommand": "bun install",
  "buildCommand": "bun run build",
  "outputDirectory": "dist",
  "functions": {
    "src/main.ts": {
      "runtime": "nodejs22.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/main.ts"
    }
  ]
}