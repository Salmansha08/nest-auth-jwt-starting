{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ]
    }
  ],
  "crons": [],
  "framework": null,
  "devCommand": "bun run start:dev",
  "installCommand": "bun install",
  "buildCommand": "bun run build",
  "outputDirectory": "dist",
  "regions": [
    "sin1"
  ]
}